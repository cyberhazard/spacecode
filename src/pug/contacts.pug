extends _templates/_main

block vars
  - var pageTitle = 'Контакты'
  - var customHeader = true
  - var constacts = true
  - var headerHeight = 500
  - var customBg = '/img/services-bg.png'
block header
  .g-wrapper
    .promotion
      .promotion__title Контакты
block content
  .feedback
    img(src='../img/contacts-planet.png').bg__planet_contacts
    .g-wrapper
      .feedback__block
        .feedback__grid.contacts__grid
          .feedback__item
            .feedback__name Наш адрес:
            .feedback__text г. Москва, Ул. Большая Серпуховская 44, оф 414
          .feedback__item
            .feedback__name Наш телефон:
            .feedback__text
              a.feedback__mobile(href="tel:+74951340178") +7 (495) 134-01-78
          .feedback__item
            .feedback__name Наш e-mail:
            a(href="mailto:info@spacecode.ru").feedback__text.feedback__email info@spacecode.ru
        .feedback__grid
          form.feedback__form
            .feedback__title.feedback__center Задайте нам вопрос и мы с удовольствием на него ответим!
            input.feedback__input(placeholder="Ваше имя...")
            input.feedback__input(placeholder="Ваше телефон или e-mail...")
            input.feedback__input(placeholder="Ваше сообщение...")
            .feedback__submit.exclude <span>Отправить</span>
  .contacts__map
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2247.058012623545!2d37.624587315778285!3d55.722743980545346!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x46b54b1701673375%3A0x5c4a4d737f19f41e!2z0JHQvtC70YzRiNCw0Y8g0KHQtdGA0L_Rg9GF0L7QstGB0LrQsNGPINGD0LsuLCA0NCwg0JzQvtGB0LrQstCwLCAxMTUwOTM!5e0!3m2!1sru!2sru!4v1505837628309" width="100%" height="595px" frameborder="0" style="border:0" allowfullscreen></iframe>
  .clients
    .g-wrapper
      .shadow-text(data-text="Наши клиенты") Наши клиенты
