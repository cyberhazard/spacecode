          td.bonuses__table_td До 100 000 руб.
          td.bonuses__table_td До 20 000 руб.
          td.bonuses__table_td 10%
        tr.bonuses__table_tr
          td.bonuses__table_td 100 000 - 300 000 руб.
          td.bonuses__table_td 20 000 - 60 000 руб.
          td.bonuses__table_td 15%
        tr.bonuses__table_tr
          td.bonuses__table_td 300 000 - 500 000 руб.
          td.bonuses__table_td 60 000 - 100 000 руб.
          td.bonuses__table_td 20%
        tr.bonuses__table_tr
          td.bonuses__table_td Больше 500 000 руб.
          td.bonuses__table_td Больше 100 000 руб.
