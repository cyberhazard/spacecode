.request
  .g-wrapper
    .request__content
      .request__title Хотите стать нашим партнером?
      .request__text Заполните форму и наши менеджеры в ближайшее время свяжутся с Вами
      form.part__form
        .part__item
          input.part__input(type="text" name="name" required)
          label.part__label Ваше имя*
        .part__item
          input.part__input(type="tel" name="tel" required)
          label.part__label Ваш телефон*
        .part__item
          checkbox.part__checkbox
        .part__item
          button.feedback__submit.feedback__submit_float.exclude <span>Отправить заявку</span>
  img(src="./img/request-particle-1.png").request__particle.request__particle_1
  img(src="./img/particle-request-2.png").request__particle.request__particle_2
  img(src="./img/particle-request-3.png").request__particle.request__particle_3
